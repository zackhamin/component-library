{
	"name": "components-library",
	"version": "2.0.1",
	"main": "lib/cjs/src/index.js",
	"module": "lib/esm/src/index.js",
	"types": "./lib/src/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"component:create": "hygen component new",
		"storybook": "start-storybook -p 6006",
		"build:storybook": "build-storybook",
		"build:npm": "rm -rf lib && rollup -c",
		"svgr": "svgr --icon --title-prop --replace-attr-values '#333=currentColor' --svg-props 'focusable=false' --svg-props 'aria-hidden=true' --typescript --index-template ./svgr/index-template.js --template ./svgr/template.js -d src/components/Atoms/Icons src/components/Atoms/Icons/assets",
		"qa:lint": "eslint --fix --ignore-path .gitignore .",
		"qa:prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx)\"",
		"qa": "npm run qa:lint && npm run qa:prettier",
		"test": "jest",
		"test:ci": "jest --ci",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"generate-barrels": "node ./scripts/barrel.js",
		"prepare": "husky install"
	},
	"dependencies": {
		"@react-aria/calendar": "^3.0.0",
		"classnames": "^2.3.1",
		"focus-trap-react": "^9.0.2",
		"react-aria": "^3.19.0",
		"react-paginate": "^8.1.3",
		"react-stately": "^3.17.0",
		"use-resize-observer": "^9.0.0"
	},
	"overrides": {
		"ejs": "^3.1.7"
	},
	"peerDependencies": {
		"react": "^17.0.0 || ^18.0.0",
		"react-dom": "^17.0.0 || ^18.0.0",
		"styled-components": "^5.3.3"
